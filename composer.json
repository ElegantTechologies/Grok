{
    "name": "eleganttechnologies/grok",
    "description": "Simple Embedded Explanations",
    "license": "mit",
    "authors": [
        {
            "name": "JJ Rohrer",
            "email": "jj.github@rohrer.org",
            "homepage": "https://www.eleganttechnologies/aboutme"
        }
    ],
    "homepage": "https://github.com/eleganttechnologies/grok",
    "keywords": ["Laravel", "Grok"],
    "require": {
        "illuminate/support": "~5|~6|~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0|~9.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4|~5|~6",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ElegantTechnologies\\Grok\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElegantTechnologies\\Grok\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ElegantTechnologies\\Grok\\GrokServiceProvider"
            ],
            "aliases": {
                "Grok": "ElegantTechnologies\\Grok\\Facades\\Grok"
            }
        }
    }
}
